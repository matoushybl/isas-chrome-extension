{
    "manifest_version": 2,
    "name": "KGK weby",

    "description": "Rozšíření zjednodušující přístup k webům Klvaňova gymnázia",
    "version": "1.0.1",

    "icons": {
        "16": "icon.png",
        "48": "icon48.png",
        "128": "icon.png"
    },

    "permissions": ["tabs"],

    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}
